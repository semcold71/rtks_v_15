<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="391.0" prefWidth="600.0" stylesheets="@../styles/style.css" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml" fx:controller="ru.samcold.rtks.controllers.print.PrintController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" styleClass="title-label" text="Сохранение документа (MS Word))"/>
        <VBox layoutX="14.0" layoutY="52.0" spacing="20.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0"
              AnchorPane.topAnchor="76.0">
            <children>
                <HBox alignment="TOP_CENTER" spacing="20.0">
                    <children>
                        <RadioButton fx:id="contract_rb" mnemonicParsing="false" text="Договор"/>
                        <RadioButton fx:id="order_rb" mnemonicParsing="false" text="Счет"/>
                        <RadioButton fx:id="act_rb" mnemonicParsing="false" text="Акт"/>
                    </children>
                </HBox>
                <AnchorPane fx:id="container_pane" prefHeight="200.0" prefWidth="200.0"/>
            </children>
        </VBox>
        <Button fx:id="cancel_btn" cancelButton="true" layoutX="516.0" layoutY="508.0" mnemonicParsing="false"
                text="Отмена" AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="14.0"/>
    </children>
</AnchorPane>
